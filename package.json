{
  "name": "auto-libs",
  "version": "0.1.24",
  "description": "凹凸 公用组件",
  "main": "build/index",
  "types": "build/index",
  "repository": "git@github.com:shaodahong/auto-lib.git",
  "author": "shaodahong <337097324@qq.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development PACKAGE=development node scripts/start.js",
    "build": "cross-env NODE_ENV=production PACKAGE=production node scripts/build.js",
    "release": "sh scripts/release.sh",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node scripts/verify-commit-msg.js"
    }
  },
  "files": [
    "build",
    "types"
  ],
  "dependencies": {
    "at-js-sdk": "^2.2.2",
    "axios": "^0.18.0",
    "qs": "^6.5.2",
    "weixin-js-sdk": "^1.4.0-test"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "commitizen": "^3.0.5",
    "conventional-changelog": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "git-cz": "^1.8.0",
    "gulp": "^4.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-typescript": "^5.0.0",
    "husky": "^1.3.1",
    "merge2": "^1.2.3",
    "tslint": "^5.12.1",
    "tslint-config-alloy": "^0.2.1",
    "typescript": "^3.2.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
