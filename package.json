{
  "name": "auto-libs",
  "version": "0.0.11",
  "description": "凹凸 公用组件",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "git@github.com:shaodahong/auto-lib.git",
  "author": "shaodahong <337097324@qq.com>",
  "license": "MIT",
  "scripts": {
    "start": "tsc -p tsconfig.build.json -w",
    "build": "rm -rf build/ && tsc -p tsconfig.build.json",
    "release": "sh scripts/release.sh",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node scripts/verify-commit-msg.js"
    }
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "at-js-sdk": "^2.2.2",
    "axios": "^0.18.0",
    "qs": "^6.5.2",
    "weixin-js-sdk": "^1.4.0-test"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "commitizen": "^3.0.5",
    "conventional-changelog": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "git-cz": "^1.8.0",
    "husky": "^1.3.1",
    "typescript": "^3.2.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
